{
  "compilerOptions": {
      "outDir": "./.tsbuild/",
      "sourceMap": false,
      "target": "esnext",
      "module": "commonjs",
      "removeComments": true,
      "noImplicitAny": true,
      "strictPropertyInitialization": true,
      "strictNullChecks": true,
      "moduleResolution": "node",
      "allowJs": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
  },
  "exclude": ["node_modules"]
}
